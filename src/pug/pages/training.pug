include  ../utils/_mixins

- var current = "training";

doctype html
html(lang='en')
    head
        meta(charset='UTF-8')
        meta(name='viewport' content='width=device-width, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0')
        meta(name="description", content=description)
        title #{title}
        block head

    body
        include ../includes/modules/_header
        section#training
            .container
                .training
                    include ../libs/trainingHelpers/training-head
                    include ../libs/trainingHelpers/training-helpers
                    include ../libs/trainingHelpers/training-block
                    include ../libs/trainingHelpers/training-finish
                    include ../libs/_regsitration
        include ../includes/modules/_footer
    block script